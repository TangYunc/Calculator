<!--pages/cal/cal.wxml-->
<view class='content'>
  <image class='imageContainer' src='../assets/image/bg.png'>

  </image>
  <view class='screen'>
    {{screenData}}
  </view>
  <view class='btnGroup'>
    <view class='item orange' catchtap='clickBtn' id='{{id1}}'>退格</view>
    <view class='item orange' catchtap='clickBtn' id='{{id2}}'>清屏</view>
    <view class='item orange' catchtap='clickBtn' id='{{id3}}'>+/-</view>
    <view class='item orange' catchtap='clickBtn' id='{{id4}}'>+</view>
  </view>
  <view class='btnGroup'>
    <view class='item blue' catchtap='clickBtn' id='{{id5}}'>9</view>
    <view class='item blue' catchtap='clickBtn' id='{{id6}}'>8</view>
    <view class='item blue' catchtap='clickBtn' id='{{id7}}'>7</view>
    <view class='item orange' catchtap='clickBtn' id='{{id8}}'>-</view>
  </view>
  <view class='btnGroup'>
    <view class='item blue' catchtap='clickBtn' id='{{id9}}'>6</view>
    <view class='item blue' catchtap='clickBtn' id='{{id10}}'>5</view>
    <view class='item blue' catchtap='clickBtn' id='{{id11}}'>4</view>
    <view class='item orange' catchtap='clickBtn' id='{{id12}}'>x</view>
  </view>
  <view class='btnGroup'>
    <view class='item blue' catchtap='clickBtn' id='{{id13}}'>3</view>
    <view class='item blue' catchtap='clickBtn' id='{{id14}}'>2</view>
    <view class='item blue' catchtap='clickBtn' id='{{id15}}'>1</view>
    <view class='item orange' catchtap='clickBtn' id='{{id16}}'>÷</view>
  </view>
  <view class='btnGroup'>
    <view class='item blue' catchtap='clickBtn' id='{{id17}}'>0</view>
    <view class='item blue' catchtap='clickBtn' id='{{id18}}'>.</view>
    <view class='item blue' catchtap='history' id='{{id19}}'>历史</view>
    <view class='item orange' catchtap='clickBtn' id='{{id20}}'>=</view>
  </view>
</view>